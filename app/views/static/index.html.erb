 <div class = "image">
    <img src="https://i.imgur.com/rdFz3Ah.png">
 </div>
 <div class = home-buttons>
     <div class = "create-user">
         <%= link_to "Create User", new_user_path(), class: "button"%>
     </div>
     <div class = "login">
         <%= link_to "Login", login_path(), class: "button"%>
     </div>
     <div class = "login-guest">
         <%= form_tag login_path do%>
         <input type="hidden" name="login[username]" value="guest"/>
         <input type="hidden" name="login[password]" value="1"/>
         <input type="submit"  value="Login as Guest" class="button">
         <%end%>
     </div>
 </div>
