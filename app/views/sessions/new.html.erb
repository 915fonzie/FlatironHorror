<div class= "form">
  <%= form_for :login do |f| %>
    <div>
      <%= f.label :username %>
      <%= f.text_field :username, autofocus: true%>
    </div>
    <div>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <div>
      <%= f.submit "Log In", class: "button" %>
    </div>
  <% end %>
</div>